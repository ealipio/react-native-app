{
  "expo": {
    "name": "Cod3a",
    "slug": "Cod3a",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "android": {
      "package": "com.cod3a.peru",
      "permissions": []
    },
    "orientation": "default",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "description": "a marketplace for selling the stuff you don't need anymore."
  }
}
